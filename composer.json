{
	"name":             "pattern-lab/styleguidekit-mustache-default",
	"description":      "The default Mustache-based styleguide mark-up and styles for Pattern Lab",
	"keywords":         ["mustache", "pattern lab", "styleguide"],
	"homepage":         "http://patternlab.io",
	"license":          "MIT",
	"type":             "patternlab-styleguidekit",
	"authors": [
		{
			"name":         "Dave Olsen",
			"email":        "dmolsen@gmail.com",
			"homepage":     "http://dmolsen.com",
			"role":         "Lead Developer"
		},
		{
			"name":         "Brad Frost",
			"homepage":     "http://bradfrostweb.com",
			"role":         "Creator"
		}
	],
	"support": {
		"issues":         "https://github.com/pattern-lab/styleguidekit-mustache-default/issues",
		"wiki":           "http://patternlab.io/docs/",
		"source":         "https://github.com/pattern-lab/styleguidekit-mustache-default/releases"
	},
	"extra": {
		"assets": {
			"publicDir": [
				{ "bower_components/*": "styleguide/bower_components/*" },
				{ "css/styleguide.css": "styleguide/css/styleguide.css" },
				{ "css/typeahead.css": "styleguide/css/typeahead.css" },
				{ "fonts/*": "styleguide/fonts/*" },
				{ "html/index.html": "index.html" },
				{ "images/*": "styleguide/images/*" },
				{ "js/*": "styleguide/js/*" }
			],
			"sourceDir": [
				{ "css/styleguide-specific.css": "styleguide/css/styleguide-specific.css" }
			]
		},
		"config": [
			{ "styleguideKit": "pattern-lab/styleguidekit-mustache-default" }
		]
	}
}